<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bluekjg.admin.mapper.OrderAddressMapper">
<!-- 添加快递单号 -->
	<update id="insertExpress" parameterType="OrderAddress">
		update t_evm_order_address set EXPRESS_NO=#{expressNo},`NOTE` = #{note}
		where is_del=0 and order_id=#{orderId}
	</update>
</mapper>
